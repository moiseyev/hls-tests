{
    "proxyPort": 9000,
    "capabilities": {
        "name": "HLS Native Support",
        "build": "1",
        "browserstack.local": false,
        "fixSessionCapabilities": false
    },
    "defaultTimeout": 300000,
    "environments": [
        { "browser": "Safari" }
    ],
    "maxConcurrency": 4,
    "tunnel": "browserstack",
    "tunnelOptions": {
        "verbose": true
    },
    "loaderOptions": {
        "packages": null
    },
    "suites": null,
    "functionalSuites": "build/tests/functional/*.js"
}